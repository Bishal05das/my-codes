#include <bits/stdc++.h>
using namespace std;
 
int sqrt(int x){
    int l=1,r=x;
    while(l<=r){
    int m=l+(r-l)/2;
    cout<<m<<'\n';
     if(m*m==x) return m;
     if(m*m>x) r=m-1,cout<<r<<'\n';
     else l=r+1,cout<<l<<'\n';
    }
    return -1;
}
int main()
{

int z=sqrt(64);
if(z==-1) cout<<"error"<<'\n';
else cout<<z<<'\n';





}
