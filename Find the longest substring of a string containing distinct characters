#include<bits/stdc++.h>
using namespace std;

int main(){
ios_base::sync_with_stdio(false);
cin.tie(0);
string s,a;
cin>>s;
int begin=0,end=0;
set<char>ch;
for(int low=0,high=0;high<s.size();high++){
if(ch.count(s[high])==0){
    ch.insert(s[high]);

}else{
  if(end-begin<(high-1)-low){
    begin=low;
    end=high-1;

  }
  while(s[low]!=s[high]){
    low++;
  }
  low++;
  ch.clear();
}
}
cout<<s.substr(begin, end - begin + 1);

}
